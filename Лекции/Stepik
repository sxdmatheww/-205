Операции с целыми числами
In [1]:2 + 5
Out[1]:7
In [2]:10 - 5
Out[2]:5
In [3]:6 * 7
Out[3]:42
In [4]:12345678 * 987654328765432318765
Out[4]:12193262318244164938266047670
In [5]:3 + 5 * 4
Out[5]:23
In [6]:(3 + 5) * 4
Out[6]:32
In [7]:40 // 8
Out[7]:5
In [8]:42 // 8
Out[8]:5
In [9]:42 % 8
Out[9]:2
In [10]:239 % 10
Out[10]:9
In [11]:239 // 10
Out[11]:23
In [12]:2 ** 5
Out[12]:32
In [13]:- (42)
Out[13]:-42
In []:+ (42)
In [15]:+-+42
Out[15]:-42

